{
  "id": "celestial_custodians",
  "name_key": "theme_name_celestial_custodians",
  "name_short_key": "theme_name_short_celestial_custodians",
  "name_long_key": "theme_name_long_celestial_custodians",
  "icon": "themes/celestial_custodians/images/icon_celestial_custodians.svg",
  "lore_key": "theme_lore_celestial_custodians",
  "inspiration_key": "theme_inspiration_celestial_custodians",
  "tone_key": "theme_tone_celestial_custodians",
  "concept_key": "theme_concept_celestial_custodians",
  "category_key": "theme_category_celestial_custodians",
  "style_key": "theme_style_celestial_custodians",
  "naming_convention": "asset",
  "base_attributes": {
    "integrity": 100,
    "willpower": 50,
    "aptitude": 10,
    "resilience": 10
  },
  "attribute_tooltips": {
    "integrity": "tooltip_attr_integrity_celestial_custodians",
    "willpower": "tooltip_attr_willpower_celestial_custodians",
    "aptitude": "tooltip_attr_aptitude_celestial_custodians",
    "resilience": "tooltip_attr_resilience_celestial_custodians"
  },
  "equipment_slots": {
    "custodial_implement": {
      "id": "equipped_implement_effect",
      "type": "static",
      "reward_trigger": "custodial_implement_reward_trigger"
    },
    "utility_suite": {
      "id": "equipped_utility_suite_effect",
      "type": "static",
      "reward_trigger": "utility_suite_reward_trigger"
    },
    "auxiliary_gadget": {
      "id": "equipped_auxiliary_gadget_effect",
      "type": "consumable",
      "reward_trigger": "auxiliary_gadget_reward_trigger"
    },
    "union_scrip": {
      "id": "union_scrip_balance",
      "type": "money",
      "reward_trigger": "union_scrip_reward_trigger"
    }
  },
  "dashboard_config_ref": "celestial_custodians",
  "dashboard_config": {
    "top_panel": [
      {
        "id": "integrity",
        "label_key": "char_panel_label_integrity",
        "tooltip_key": "tooltip_attr_integrity_celestial_custodians",
        "type": "meter",
        "meter_type": "health",
        "maps_to_run_stat": "currentIntegrity",
        "short_description": "Structural & Sanity Cohesion. The ability of the Custodial Unit to withstand damage, reality stress, and existential bewilderment. Zero means unscheduled, rapid disassembly and a sternly worded memo from HR."
      },
      {
        "id": "willpower",
        "label_key": "char_panel_label_willpower",
        "tooltip_key": "tooltip_attr_willpower_celestial_custodians",
        "type": "meter",
        "meter_type": "stamina",
        "maps_to_run_stat": "currentWillpower",
        "short_description": "Gumption. A finite reserve of nerve and focus. Expended to operate unreliable tech or attempt acts of improbable reasoning. Recovers with tea."
      },
      {
        "id": "aptitude",
        "label_key": "char_panel_label_aptitude",
        "tooltip_key": "tooltip_attr_aptitude_celestial_custodians",
        "type": "number",
        "maps_to_run_stat": "aptitude",
        "short_description": "Custodial Know-How. Proficiency with bizarre tools and navigating byzantine regulations."
      },
      {
        "id": "resilience",
        "label_key": "char_panel_label_resilience",
        "tooltip_key": "tooltip_attr_resilience_celestial_custodians",
        "type": "number",
        "maps_to_run_stat": "resilience",
        "short_description": "Existential Insulation. Innate ability to passively endure cosmic absurdity and reality-warping events."
      },
      {
        "id": "strain_level",
        "label_key": "label_strain_level_celestial_custodians",
        "tooltip_key": "tooltip_item_strain_level_celestial_custodians",
        "type": "status_icon",
        "default_ai_value": 2,
        "maps_to_run_stat": "strainLevel",
        "level_mappings": {
          "1": {
            "display_text_key": "strain_level_1_display_cc",
            "css_class": "status-ok",
            "icon_path": "images/sentiment/icon_sentiment_satisfied.svg"
          },
          "2": {
            "display_text_key": "strain_level_2_display_cc",
            "css_class": "status-warning",
            "icon_path": "images/sentiment/icon_sentiment_neutral.svg"
          },
          "3": {
            "display_text_key": "strain_level_3_display_cc",
            "css_class": "status-danger",
            "icon_path": "images/sentiment/icon_sentiment_dissatisfied.svg"
          },
          "4": {
            "display_text_key": "strain_level_4_display_cc",
            "css_class": "status-critical",
            "icon_path": "images/sentiment/icon_sentiment_very_dissatisfied.svg"
          }
        },
        "must_translate": false,
        "short_description": "Exasperation. The Custodian's mental fatigue. MUST BE INTEGER (1-4). 1: Placid, 2: Mildly Perturbed, 3: Thoroughly Baffled, 4: On the Verge of a Stiff Drink."
      }
    ],
    "left_panel": [
      {
        "id": "vessel_core_systems",
        "title_key": "title_vessel_core_systems_cc",
        "type": "collapsible",
        "initial_expanded": true,
        "items": [
          {
            "id": "ship_name",
            "label_key": "label_ship_name_celestial_custodians",
            "tooltip_key": "tooltip_item_ship_name_celestial_custodians",
            "type": "text",
            "default_value_key": "ship_name_default_celestial_custodians",
            "must_translate": false,
            "short_description": "Designation of the Custodian's vessel."
          },
          {
            "id": "ai_assistant_name",
            "label_key": "label_ai_assistant_name_cc",
            "tooltip_key": "tooltip_item_ai_assistant_name_cc",
            "type": "text",
            "default_value_key": "ai_name_default_cc",
            "must_translate": false,
            "short_description": "The designation of the ship's ever-present, often sarcastic, AI companion."
          },
          {
            "id": "ai_rapport_level",
            "label_key": "label_ai_rapport_level_cc",
            "tooltip_key": "tooltip_item_ai_rapport_level_cc",
            "type": "status_level",
            "default_ai_value": 2,
            "level_mappings": {
              "1": {
                "display_text_key": "rapport_level_1_display_cc",
                "css_class": "status-critical"
              },
              "2": {
                "display_text_key": "rapport_level_2_display_cc",
                "css_class": "status-warning"
              },
              "3": {
                "display_text_key": "rapport_level_3_display_cc",
                "css_class": "status-ok"
              },
              "4": {
                "display_text_key": "rapport_level_4_display_cc",
                "css_class": "status-excellent"
              }
            },
            "must_translate": false,
            "short_description": "The AI's current opinion of you. Integer (1-4). Influences its helpfulness. 1: Actively Disappointed, 2: Professionally Distant, 3: Begrudgingly Cooperative, 4: Unsettlingly Chummy."
          },
          {
            "id": "ai_personality_quirk",
            "label_key": "label_ai_personality_quirk_cc",
            "tooltip_key": "tooltip_item_ai_personality_quirk_cc",
            "type": "text",
            "default_value_key": "quirk_default_cc",
            "must_translate": true,
            "short_description": "The AI's current dominant personality trait or fixation (e.g., 'Obsessed with Biscuits', 'Aspiring Stand-Up Comedian')."
          }
        ]
      },
      {
        "id": "personnel_file_and_loadout",
        "title_key": "title_personnel_file_cc",
        "type": "collapsible",
        "initial_expanded": true,
        "items": [
          {
            "id": "union_scrip_balance",
            "label_key": "label_union_scrip_celestial_custodians",
            "tooltip_key": "tooltip_item_union_scrip_celestial_custodians",
            "type": "number_text",
            "default_value_key": "union_scrip_default_celestial_custodians",
            "must_translate": true,
            "short_description": "Thematic currency. Balance of Union Scrip."
          },
          {
            "id": "equipped_implement_effect",
            "label_key": "label_equipped_implement_celestial_custodians",
            "tooltip_key": "tooltip_slot_custodial_implement_celestial_custodians",
            "type": "text_long",
            "default_value_key": "default_equipped_implement_celestial_custodians",
            "must_translate": true,
            "short_description": "Name and effect of the equipped primary Custodial Implement."
          },
          {
            "id": "equipped_utility_suite_effect",
            "label_key": "label_equipped_utility_suite_celestial_custodians",
            "tooltip_key": "tooltip_slot_utility_suite_celestial_custodians",
            "type": "text_long",
            "default_value_key": "default_equipped_utility_suite_celestial_custodians",
            "must_translate": true,
            "short_description": "Name and effect of the active Utility Suite or Multi-Tool."
          },
          {
            "id": "equipped_auxiliary_gadget_effect",
            "label_key": "label_equipped_auxiliary_gadget_celestial_custodians",
            "tooltip_key": "tooltip_slot_auxiliary_gadget_celestial_custodians",
            "type": "text_long",
            "default_value_key": "default_equipped_auxiliary_gadget_celestial_custodians",
            "must_translate": true,
            "short_description": "Name and effect of the deployed Auxiliary Gadget or Prototype."
          }
        ]
      }
    ],
    "right_panel": [
      {
        "id": "union_work_order_panel",
        "title_key": "title_union_work_order_cc",
        "type": "collapsible",
        "initial_expanded": true,
        "items": [
          {
            "id": "assignment_brief_text",
            "label_key": "label_assignment_brief_celestial_custodians",
            "tooltip_key": "tooltip_item_assignment_brief_celestial_custodians",
            "type": "text_long",
            "default_value_key": "assignment_brief_standby_celestial_custodians",
            "must_translate": true,
            "short_description": "The primary objective of the current cosmic cleanup, bewildering errand, or request to 'just make the noise stop'."
          },
          {
            "id": "work_order_id",
            "label_key": "label_work_order_id_cc",
            "tooltip_key": "tooltip_item_work_order_id_cc",
            "type": "text",
            "default_value_key": "work_order_id_none_cc",
            "must_translate": false,
            "short_description": "The official, likely nonsensical, Union identification code for the current assignment."
          },
          {
            "id": "stated_compensation",
            "label_key": "label_stated_compensation_cc",
            "tooltip_key": "tooltip_item_stated_compensation_cc",
            "type": "text",
            "default_value_key": "compensation_none_cc",
            "must_translate": true,
            "short_description": "The stated payment upon successful (or at least, not catastrophically unsuccessful) completion of the work order."
          }
        ]
      },
      {
        "id": "hazard_anomaly_assessment_panel",
        "title_key": "title_hazard_assessment_cc",
        "type": "collapsible",
        "initial_expanded": true,
        "items": [
          {
            "id": "cosmic_weirdness_index_text",
            "label_key": "label_cosmic_weirdness_index_celestial_custodians",
            "tooltip_key": "tooltip_item_weirdness_index_celestial_custodians",
            "type": "status_level",
            "default_ai_value": 2,
            "level_mappings": {
              "1": { "display_text_key": "weirdness_index_mundane_celestial_custodians", "css_class": "status-ok" },
              "2": { "display_text_key": "weirdness_index_peculiar_celestial_custodians", "css_class": "status-warning" },
              "3": { "display_text_key": "weirdness_index_bizarre_celestial_custodians", "css_class": "status-warning" },
              "4": { "display_text_key": "weirdness_index_reality_bending_celestial_custodians", "css_class": "status-danger" }
            },
            "must_translate": false,
            "short_description": "Overall measure of situational strangeness. CRUCIAL: MUST be ONLY a numerical string: \"1\" (Surprisingly Normal), \"2\" (Mildly Peculiar), \"3\" (Actively Bizarre), or \"4\" (Reality is Optional)."
          },
          {
            "id": "local_anomaly_report",
            "label_key": "label_anomaly_report_celestial_custodians",
            "tooltip_key": "tooltip_item_anomaly_report_celestial_custodians",
            "type": "text_long",
            "default_value_key": "anomaly_report_clear_celestial_custodians",
            "must_translate": true,
            "short_description": "Live report of unusual local phenomena or bizarre contaminants. This panel is always visible but content changes."
          }
        ]
      },
      {
        "id": "navigational_data_panel",
        "title_key": "title_navigational_data_cc",
        "type": "collapsible",
        "initial_expanded": false,
        "items": [
          {
            "id": "current_location",
            "label_key": "label_current_location_cc",
            "tooltip_key": "tooltip_item_current_location_cc",
            "type": "text_long",
            "default_value_key": "location_unknown_cc",
            "must_translate": true,
            "short_description": "The ship's current registered position in the cosmos, according to its (possibly whimsical) navigation system."
          },
          {
            "id": "local_time_perception",
            "label_key": "label_local_time_perception_cc",
            "tooltip_key": "tooltip_item_local_time_perception_cc",
            "type": "text",
            "default_value_key": "time_nominal_cc",
            "must_translate": true,
            "short_description": "The perceived flow of time in the immediate vicinity, which is not always consistent."
          },
          {
            "id": "existential_bearing",
            "label_key": "label_existential_bearing_cc",
            "tooltip_key": "tooltip_item_existential_bearing_cc",
            "type": "text",
            "default_value_key": "bearing_unclear_cc",
            "must_translate": true,
            "short_description": "A deeply philosophical, often unhelpful, assessment of the Custodian's current purpose or place in the grand, baffling scheme of things."
          }
        ]
      }
    ],
    "game_state_indicators": [
      {
        "id": "custodial_implement_reward_trigger",
        "type": "boolean",
        "default_value": false,
        "short_description": "Backend trigger. Set to true to signal a Custodial Implement reward is available."
      },
      {
        "id": "utility_suite_reward_trigger",
        "type": "boolean",
        "default_value": false,
        "short_description": "Backend trigger. Set to true to signal a Utility Suite reward is available."
      },
      {
        "id": "auxiliary_gadget_reward_trigger",
        "type": "boolean",
        "default_value": false,
        "short_description": "Backend trigger. Set to true to signal an Auxiliary Gadget reward is available."
      },
      {
        "id": "union_scrip_reward_trigger",
        "type": "boolean",
        "default_value": false,
        "short_description": "Backend trigger. Set to true to signal a Union Scrip currency reward is available."
      }
    ]
  }
}
